 <script>

		$(function(){

			$('.recent_statistics_champion_icon').mouseenter(function(){ $(this).children().first().css('opacity', 0.3); }).mouseleave(function(){ $(this).children().first().css('opacity', 0.5); })

			$('.recent_statistics_meta_bar').each(function() {

				var wins = parseInt($(this).attr('data-wins'));
				var losses = parseInt($(this).attr('data-losses'));
				var total = wins+losses;
				var winpct = total ? Math.round(100*(wins/total), 1) : 0;
				var losepct = total ? Math.round(100*(losses/total), 1) : 0;
				var totalpct = 11 ? Math.round(total/11*100, 1) : 0;


			$(this).tipsy({fallback: "<table style='font-size: 14px;'><tr><td style='text-align: right;'><strong>Wins</strong>:</td><td style='text-align: left;'>" + wins + " (" + winpct  + "%)</td></tr><tr><td style='text-align: right;'><strong>Losses</strong>:</td><td style='text-align: left;'>" + losses + " (" + losepct  + "%)</td></tr><tr><td style='text-align: right;'><strong>Games Played</strong>:</td><td style='text-align: left;'>" + total + " (" + totalpct + "%)</td></tr></table>", gravity: 'w', fade: true, offset: 4, html: true});

			$(this).mouseenter(function() { $(this).next().css('opacity', 1); }).mouseleave(function() { $(this).next().css('opacity', 0.8); });

		});

	});

</script>






































































































































































































































































































































































































































































































































































