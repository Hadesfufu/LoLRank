												'</div>',
											'</div>',
										'</div>'
									].join('');

									rows.push(html);

								}

							}

							$('#search_suggestions').empty().html(rows.join(''));

							if( $('#search_suggestions').children().length > 0 ) {
								$('#search_suggestions').fadeIn();
							} else {
								$('#search_suggestions').hide();
							}

						});


						$('#search_input_box').focus(function(){
						
							$('#hide_regions_btn').tipsy("show");
						
						});
						
						$('#search_input_box').blur(function(){
						
							$('#hide_regions_btn').tipsy("hide");
						
						});				
						
						//$('#hide_regions_btn').tipsy({fallback:"Click here to show and hide regions!", gravity: 'nw', fade: true, offset: -4});
						
						$('#search-settings-save-btn').live('click', function(e){

							e.preventDefault();
							
							var selected_regions = $('#fancybox-content [name="enabled-search-region"]:checked').map(function(i, el){
							
								return $(this).val();
							
							}).get();
							
							if( selected_regions.length > 0 ) {
							
								LOLKING.createCookie('enabled-search-regions', selected_regions.join(','), 60*60*24*365);
								window.location.reload(true);
							
							} else {
							
								$('#fancybox-content .notification').css('color', '#F44').text('Error: Please select at least 1 region.');
							
							}

						});
												$('#hide_regions_btn').click(function(e){

