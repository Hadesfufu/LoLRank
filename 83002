 <script>

		$(function(){

			$('.ranked_stats_season_tabs').each(function(){

				$(this).find('a').click(function(e){

					var hash = $(this).attr('href');

					if( hash.charAt(0) == '#' || hash.charAt(0) == '.' ) {
						e.preventDefault();

						$tab = $(hash);
						$tab.parent().children().hide();
						$tab.show();

						$(this).addClass('selected').siblings().removeClass('selected');
					}

				}).last().trigger('click');

			});

		});

	</script>










































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































                       <script>
    $(function () {
        var currentTab = '';
        var isRanked = $('#hash-leagues').length > 0;

        /**
         * Record visitation once we can determine which tab is open and whether or not
         * the summoner is ranked.
         */
        (function trackSummonerVisit() {
            if ($('.tabs2 > .selected > a').length > 0) {
                currentTab = $('.tabs2 > .selected > a').text();
                LOLKING.log.custom('Summoner (' + (isRanked ? 'Ranked' : 'Unranked') + ')', currentTab, window.location.pathname);
            } else {
                setTimeout(trackSummonerVisit, 100);
            }
        })();

        /**
         * Record interactions with page tabs.
         */
        (function () {
            $('.tabs2 > li > a').click(function () {
                var clickedTab = $(this).text();
                if (currentTab !== '' && clickedTab !== currentTab) {
                    LOLKING.log.custom('Summoner (' + (isRanked ? 'Ranked' : 'Unranked') + ')', clickedTab, window.location.pathname);
                    currentTab = clickedTab;
                }
            });
        })();
    });
</script>



























































     <script>
					
						$(function(){

