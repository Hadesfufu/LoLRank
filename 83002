










 <script>
		
		window.LOLKING = {};
		
		LOLKING.createCookie = function(name, val, time) {
			var expires = "";
			if( time ) {
				var date = new Date();
				date.setTime(date.getTime()+(time*1000));
				expires = "; expires="+date.toGMTString();
			}

			document.cookie = name+"="+val+expires+"; path=/; domain=.lolking.net";
		}

		LOLKING.getCookie = function(name) {
			name += '=';
			var parts = document.cookie.split(/;\s*/);
			for (var i = 0; i < parts.length; i++)
			{
				var part = parts[i];
				if (part.indexOf(name) == 0)
					return part.substring(name.length)
			}
			return null;
		}

		LOLKING.log = new function() {
	        this.view = function(category, label) {
	            if (category && label) {
	                if (typeof _gaq != 'undefined') {
	                    _gaq.push(['_trackEvent', category, 'Impression', label]);
	                } else {
	                    setTimeout(function() { LOLKING.log.view(category, label) }, 100);
	                }
	            }
	        }
	
	        this.click = function(category, label) {
	            if (category && label) {
	                if (typeof _gaq != 'undefined') {
	                    _gaq.push(['_trackEvent', category, 'Clickthrough', label]);
	                } else {
	                    setTimeout(function() { LOLKING.log.click(category, label) }, 100);
	                }
	            }
	        }

            /**
             * Log a custom track event in GA. Will keep trying until GA is available.
             *
             * @param category
             * @param action
             * @param label
             */
            this.custom = function (category, action, label) {
                if (category && action && label) {
                    if (typeof _gaq != 'undefined') {
                        _gaq.push(['_trackEvent', category, action, label]);
                    } else {
                        setTimeout(function () { LOLKING.log.custom(category, action, label) }, 100);
                    }
                }
            }
	    };

		// left: 37, up: 38, right: 39, down: 40,
		// spacebar: 32, pageup: 33, pagedown: 34, end: 35, home: 36

		function preventDefault(e) {
		  e = e || window.event;
		  if (e.preventDefault)
			  e.preventDefault();
		  e.returnValue = false;  
		}

		function wheel(e) {
		  preventDefault(e);
		}

		function disable_scroll() {
		  if (window.addEventListener) {
			  window.addEventListener('DOMMouseScroll', wheel, false);
		  }
		  window.onmousewheel = document.onmousewheel = wheel;
		  //document.onkeydown = keydown;
		}

		function enable_scroll() {
			if (window.removeEventListener) {
				window.removeEventListener('DOMMouseScroll', wheel, false);
			}
			window.onmousewheel = document.onmousewheel = document.onkeydown = null;  
		}		
		
	</script>



























































 <script>
	
		jQuery.fn.center = function () {
			this.css("position","absolute");
			this.css("top", Math.max(0, (($(window).height() - this.outerHeight()) / 2) + $(window).scrollTop()) + "px");
			this.css("left", Math.max(0, (($(window).width() - this.outerWidth()) / 2) + $(window).scrollLeft()) + "px");
			return this;
		}
		
		
		window.alert = function(msg) {

			$('#notification-shim, #notification').remove();
			$('body').append('<div id="notification-shim" style=""></div>');
			$('body').append([
				'<div class="pane" id="notification">',
					'<div id="notification-content">',
						msg,
						'<div id="notification-buttonbar">',
							'<button class="blue" onclick="$(\'#notification-shim\').click();">Close</button>',
						'</div>',
					'</div>',
				'</div>'
			].join(''));
	
			$('#notification').center();
			$('#notification-shim').show();
			$('#notification').fadeIn(500);
		
			$('#notification-shim').click(function(){
			
				$('#notification-shim, #notification').remove();
			
			});
		
		}

        $(function(){
           //Set focus on search box so user can start typing a name when page loads
            if(!window.location.hash){
                $("#search_input_box").focus();
            }
        });
		
	</script>













    <script>
    var ads = new function() {
        var scriptCalled = false;
        var queue = [];

        this.load = function(id, channelId) {
            queue.push({
                auid: id,
                slot_id: 'slot' + id,
                tid: channelId
            });
            ads.init();
        };

        this.init = function() {
            if (!scriptCalled) {
                scriptCalled = true;
                $.getScript('http://ox-d.zam.com/w/1.0/jstag');
            }
            if (typeof OX == 'undefined') {
                setTimeout(ads.init, 50);
                return;
            }
            window.ox_instances = window.ox_instances || {};
            var ad, ad_i = window['ox_instances'], inst;
            while((ad = queue.shift())){
                inst = ad_i[ad.auid] = OX(ad);
                inst.addHook(ads.onLoad, OX.Hooks.ON_ADUNIT_INITIALIZED);
                inst.load();
            }
        };

        this.onLoad = function(unit) {
            var r_advs = {'':1,119398:1,118043:1},
                ns = 'ox_instances',
                inst = window[ns] && window[ns][unit.get('adunit_id')] || null,
                adv_id = unit.get('adv_id'),
                adv_id = adv_id.substr(adv_id.indexOf(':') + 1);
                
            if ( !( adv_id in r_advs || unit.get('adv_id').indexOf('-') > -1 ) ) {
                if (inst) inst.setRefreshMax(0);
            }
                    };
    };
    </script>




































    <script>
	
    var skinSplitLoaded = false;
	function addSkinSplit() {
		$('body').css('background-color', '#000');
		$detach1 = $('#container').children().first().detach();
		$('#container').css('margin-top', '182px').prepend('<div style="height: 10px;"></div>');
		$detach2 = $('#container #container_inner').children().slice(0, 2).detach();
		$('#container').parent().prepend($('<div id="container" class="container-1"><div id="container_inner" class="container_inner-1"></div><div style="height: 5px;"></div></div>'));
		$('.container-1').prepend($detach1);
		$('.container_inner-1').append($detach2);
	}

    function postSkinSetup(campaignId) {
        if (campaignId && !skinSplitLoaded) {
            skinSplitLoaded = true;
			if( $.isReady ) {
				addSkinSplit();
			} else {
				$(function(){
					addSkinSplit();
				});
			}
        }
    }
    </script>






































































     <script>
					
					$(function(){

						//https://github.com/odhyan/trie/blob/master/trie.js
						Trie=function(){this.words=0;this.prefixes=0;this.children=[]};Trie.prototype={insert:function(e,t){if(e.length==0){return}var n=this,r,i;if(t===undefined){t=0}if(t===e.length){n.words++;return}n.prefixes++;r=e[t];if(n.children[r]===undefined){n.children[r]=new Trie}i=n.children[r];i.insert(e,t+1)},remove:function(e,t){if(e.length==0){return}var n=this,r,i;if(t===undefined){t=0}if(n===undefined){return}if(t===e.length){n.words--;return}n.prefixes--;r=e[t];i=n.children[r];i.remove(e,t+1)},update:function(e,t){if(e.length==0||t.length==0){return}this.remove(e);this.insert(t)},countWord:function(e,t){if(e.length==0){return 0}var n=this,r,i,s=0;if(t===undefined){t=0}if(t===e.length){return n.words}r=e[t];i=n.children[r];if(i!==undefined){s=i.countWord(e,t+1)}return s},countPrefix:function(e,t){if(e.length==0){return 0}var n=this,r,i,s=0;if(t===undefined){t=0}if(t===e.length){return n.prefixes}var r=e[t];i=n.children[r];if(i!==undefined){s=i.countPrefix(e,t+1)}return s},find:function(e){if(e.length==0){return false}if(this.countWord(e)>0){return true}else{return false}},getAllWords:function(e){var t=this,n,r,i=[];if(e===undefined){e=""}if(t===undefined){return[]}if(t.words>0){i.push(e)}for(n in t.children){r=t.children[n];i=i.concat(r.getAllWords(e+n))}return i},autoComplete:function(e,t){if(e.length==0){return[]}var n=this,r,i;if(t===undefined){t=0}r=e[t];i=n.children[r];if(i===undefined){return[]}if(t===e.length-1){return i.getAllWords(e)}return i.autoComplete(e,t+1)}}

						//http://stackoverflow.com/questions/460070/detecting-mistyped-email-addresses-in-javascript#answer-460102
						levenshteinenator=function(e,t){var n;var r=e.length;var i=t.length;if(r<i){var s=e;e=t;t=s;var o=r;r=i;i=o}var u=new Array;u[0]=new Array;for(var s=0;s<i+1;s++){u[0][s]=s}for(var a=1;a<r+1;a++){u[a]=new Array;u[a][0]=a;for(var f=1;f<i+1;f++){n=e.charAt(a-1)==t.charAt(f-1)?0:1;u[a][f]=minimator(u[a-1][f]+1,u[a][f-1]+1,u[a-1][f-1]+n)}}return u[r][i]};minimator=function(e,t,n){if(e<t&&e<n)return e;if(t<e&&t<n)return t;return n}

